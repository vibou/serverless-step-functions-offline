{
  "name": "@vibou/serverless-step-functions-offline",
  "version": "3.0.0",
  "description": "Serverlesss plugin to support step function offline",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/vibou/serverless-step-functions-offline/issues"
  },
  "homepage": "https://github.com/vibou/serverless-step-functions-offline#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:beforeyoubid/serverless-step-functions-offline.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' --quiet",
    "ts": "tsc --noEmit",
    "build-and-publish": "npm run build && npm publish --access public",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/*.ts --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:watch": "babel src/*.ts --watch --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "2.29.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.40",
    "@types/serverless": "3.12.7",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "asl-types": "^1.2.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "jest-junit": "13.2.0",
    "pre-commit": "^1.1.3",
    "prettier": "2.6.2",
    "serverless": "^3.23.0",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "step functions",
    "serverless-step-functions-offline"
  ],
  "license": "MIT",
  "files": [
    "dist/**/*"
  ]
}
